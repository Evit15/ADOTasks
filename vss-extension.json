{
    "manifestVersion": 1,
    "id": "Evit15-build-release-task",
    "name": "Evit15 Build and Release Tools",
    "version": "0.0.1",
    "publisher": "Evit15",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for building/releasing with Evit15. Includes one build/release task.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/tasks.png"        
    },
    "files": [
        {
            "path": "buildandreleasetask/GenerateTestRun"
        }
    ],
    "contributions": [
        {
            "id": "generate-test-run",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildandreleasetask/GenerateTestRun"
            }
        }
    ]
}